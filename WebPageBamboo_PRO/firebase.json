{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "rewrites": [{
      "source": "/Api/Contact",
      "function": "app"
    }],
    "ignore": [
      "firebase.json",
      "WebPageBamboo_DEV",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
